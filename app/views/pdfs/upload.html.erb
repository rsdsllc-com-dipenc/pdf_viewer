<!-- app/views/pdfs/upload.html.erb -->
<h1>Upload PDF</h1>
<%= form_with url: process_pdf_pdfs_path, local: true, multipart: true do |form| %>
  <div class="mb-3">
    <%= form.label :pdf, class: "form-label" %>
    <%= form.file_field :pdf, class: "form-control" %>
  </div>
  <%= form.submit "Upload and Process", class: "btn btn-primary" %>
<% end %>
